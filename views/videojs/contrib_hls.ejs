<% layout('layout') %>

<h1><%= title %></h1>

<div class="row">
  <div class="col-sm-12">
    <p>Refer <a href="https://github.com/videojs/videojs-contrib-hls#getting-started" target="_blank">https://github.com/videojs/videojs-contrib-hls#getting-started</a>.</p>
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <video id=example-video width=600 height=300 class="video-js vjs-default-skin" controls>
      <source
              src="//solutions.brightcove.com/jwhisenant/hls/apple/bipbop/bipbopall.m3u8"
              type="application/x-mpegURL">
    </video>
    <script>
      var player = videojs('example-video');
      player.play();
    </script>
  </div>
</div>

