<% layout('layout') %>

<h1><%= title %></h1>

<div class="row">
  <div class="col-sm-12">
    <form method="post" enctype="multipart/form-data" action="/file_upload/multer_memory">
      <div class="form-group">
        <label>Text</label>
        <input type="text" name="text" value="TEXT">
      </div>

      <div class="form-group">
        <label>File</label>
        <input type="file" name="file">
      </div>

      <button type="submit" class="btn btn-info">Submit</button>
    </form>
  </div>
</div>

